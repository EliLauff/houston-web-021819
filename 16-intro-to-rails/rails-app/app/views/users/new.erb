<h2>Create User</h2>
<form method="POST" action="/users">
    <div>
        <label>Name</label>
        <input type="text" name="user[name]" />
    </div>
    <div>
        <label>Address</label>
        <input type="text" name="user[address]" />
    </div>
    <div>
        <label>Phone Number</label>
        <input type="text" name="user[phone_number]" />
    </div>
    <input type="submit"/>
</form>